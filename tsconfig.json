        "rootDir": "src",
        "declaration": true,
        "outDir": "dist",
        "module": "CommonJS",
        "target": "ES6",
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true
    },
    "exclude": [
        "src/protos.d.ts",
        "dist"
    ]
}
